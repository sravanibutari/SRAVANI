<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CustomerController</servlet-name>
  	<display-name>CustomerController</display-name>
  	<description></description>
  	<servlet-class>crm.controller.CustomerController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CustomerView</servlet-name>
  	<display-name>CustomerView</display-name>
  	<description></description>
  	<servlet-class>crm.view.CustomerView</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CustController</servlet-name>
  	<display-name>CustController</display-name>
  	<description></description>
  	<servlet-class>crm.controller.CustController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CusController</servlet-name>
  	<display-name>CusController</display-name>
  	<description></description>
  	<servlet-class>crm.controller.CusController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CustomerController</servlet-name>
  	<url-pattern>/CustomerController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CustomerView</servlet-name>
  	<url-pattern>/CustomerView</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CustController</servlet-name>
  	<url-pattern>/Controller.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CusController</servlet-name>
  	<url-pattern>/Controller.do</url-pattern>
  </servlet-mapping>
</web-app>
